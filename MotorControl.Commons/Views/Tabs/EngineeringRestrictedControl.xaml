        <TextBlock Grid.Row="11" Grid.Column="0" Text="Phase B Current Sense Offset Value (Volts)" />
        <TextBlock Grid.Row="11" Grid.Column="2" Text="655.36" />

        <TextBlock Grid.Row="12" Grid.Column="0" Text="Phase C Current Sense Offset Value (Volts)" />
        <TextBlock Grid.Row="12" Grid.Column="2" Text="655.36" />

        <TextBlock Grid.Row="13" Grid.Column="0" Text="Phase A Voltage Sense Offset Value (Volts)" />
        <TextBlock Grid.Row="13" Grid.Column="2" Text="655.36" />

        <TextBlock Grid.Row="14" Grid.Column="0" Text="Phase B Voltage Sense Offset Value (Volts)" />
        <TextBlock Grid.Row="14" Grid.Column="2" Text="655.36" />

        <TextBlock Grid.Row="15" Grid.Column="0" Text="Phase C Voltage Sense Offset Value (Volts)" />
        <TextBlock Grid.Row="15" Grid.Column="2" Text="655.36" />

        <TextBlock Grid.Row="16" Grid.Column="0" Text="Magnetizing Current (Amps)" />
        <TextBlock Grid.Row="16" Grid.Column="2" Text="655.36" />


        <TextBlock Grid.Row="0" Grid.Column="4" Text="Enable Stator Resistance Recalculation" />
        <TextBlock Grid.Row="0" Grid.Column="6" Text="3303" />

        <TextBlock Grid.Row="2" Grid.Column="4" Text="Enable Online Stator Resistance Calculation" />
        <ToggleButton Grid.Row="2" Grid.Column="6" />

        <TextBlock Grid.Row="4" Grid.Column="4" Text="Enable Offset Calibration-self clearing" />
        <TextBlock Grid.Row="4" Grid.Column="6" Text="3303" />

        <TextBlock Grid.Row="6" Grid.Column="4" Text="Enable/Disable Force Angle" />
        <ToggleButton Grid.Row="6" Grid.Column="6" />

        <TextBlock Grid.Row="8" Grid.Column="4" Text="Enable/Disable Flying Start" />
        <ToggleButton Grid.Row="8" Grid.Column="6" />

    </Grid>
</UserControl>
